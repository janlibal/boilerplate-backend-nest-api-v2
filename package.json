{
  "name": "nest-second-project",
  "version": "0.0.1",
  "description": "Nestjs Boilerplate API",
  "private": true,
  "homepage": "https://github.com/janlibal/boilerplate-backend-nest-api-v2",
  "author": {
    "name": "Jan Libal",
    "email": "jan.libal@yahoo.com"
  },
  "contributors": [
    {
      "name": "Jan Libal",
      "email": "jan.libal@yahoo.com"
    }
  ],
  "license": "UNLICENSED",
  "engines": {
    "node": ">= 14"
  },
  "keywords": [
    "nest",
    "nestjs",
    "rest",
    "restful",
    "api",
    "nestjs-boilerplate",
    "boilerplate"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "nest build",
    "rebuild": "yarn run clean && yarn run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:generate": "npx prisma generate",
    "generate-schema": "cat src/*/*/*.prisma > prisma/schema.prisma",
    "migrate:dev": "env-cmd -f src/config/env/.env.dev npx prisma migrate dev",
    "migrate:win": "dotenv -e ./src/config/env/.env.dev npx prisma migrate dev",
    "migrate:init": "dotenv -e ./src/config/env/.env.dev npx prisma --datasource-provider postgresql",
    "migrate:test": "env-cmd -f env-example-relational prisma migrate deploy",
    "migrate:deploy": "prisma migrate deploy",
    "seed:dev": "env-cmd -f src/config/env/.env.dev npx prisma db seed",
    "seed:win": "dotenv -e ./src/config/env/.env.dev npx prisma db seed",
    "seed:prod": "npx prisma db seed",
    "seed:test": "env-cmd -f env-example-relational npx prisma db seed",
    "start:win": "dotenv -e ./src/config/env/.env.dev nest start",
    "start": "NODE_ENV=dev nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:test": "NODE_ENV=test nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "infra:up": "docker compose -f docker-compose.db.yml --env-file src/config/env/.env.dev up -d",
    "infra:down": "docker compose -f docker-compose.db.yml down",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint-staged": "lint-staged",
    "test": "vitest run -c ./vitest.config.unit.mjs",
    "test:unit": "vitest -c ./vitest.config.unit.mjs",
    "test:unit:ui": "vitest -c ./vitest.config.unit.mjs --ui",
    "test:e2e": "vitest -c ./vitest.config.integration.mjs",
    "test:e2e:ci": "vitest -c ./vitest.config.ci.mjs",
    "test:e2e:docker": "docker compose -f docker-compose.test.yml --env-file env-example-relational -p tests up -d --build && docker compose -f docker-compose.test.yml -p tests exec api /opt/wait-for-it.sh -t 0 localhost:80 -- yarn run test:e2e && docker compose -f docker-compose.test.yml --env-file env-example-relational -p tests down && docker compose -f docker-compose.test.yml --env-file env-example-relational -p tests rm -svf",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.699.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^8.0.5",
    "@nestjs/terminus": "^10.2.3",
    "@prisma/client": "^6.0.1",
    "apple-signin-auth": "^1.7.6",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv-cli": "^7.4.2",
    "express-rate-limit": "^0.0.0-typescript-beta-7",
    "fb": "^2.0.0",
    "google-auth-library": "^9.11.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "ms": "^2.1.3",
    "nestjs-pino": "^4.1.0",
    "nestjs-prisma": "^0.23.0",
    "node-rsa": "^1.1.1",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino-cloudwatch": "^0.7.0",
    "pino-http": "^10.2.0",
    "pino-pretty": "^11.2.2",
    "prisma": "^6.0.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.1",
    "twitter": "^1.7.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/ms": "^0.7.34",
    "@types/node": "^22.9.4",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@types/twitter": "^1.7.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^8.0.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.1.8",
    "vitest-mock-extended": "^2.0.2"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "migrateOutput": "src/prisma/migrations/",
    "seed": "ts-node ./prisma/seeds/seed.ts"
  },
  "lint-staged": {
    "**/*.{ts,js}": ["eslint --fix --ignore-pattern '**/*.test.ts'","prettier --write"]
  }
}
